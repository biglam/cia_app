<%= form_for @city do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :population %>
  <%= f.text_field :population %>
  <br>
  <%= f.label :country_id %>
  <%= f.collection_select :country_id, Country.order(:name),:id,:name, include_blank: false %>
  <br>
  <%= f.label :photo %>
  <%= f.text_field :photo %>
  <br>
  <%= f.submit button_text %>
  <% end %>